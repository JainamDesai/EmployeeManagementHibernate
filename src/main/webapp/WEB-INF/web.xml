<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>EmployeeSaveController</servlet-name>
  	<display-name>EmployeeSaveController</display-name>
  	<description></description>
  	<servlet-class>com.controller.EmployeeSaveController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>com.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>deleteEmployee</servlet-name>
  	<display-name>deleteEmployee</display-name>
  	<description></description>
  	<servlet-class>com.controller.deleteEmployee</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EmployeeSaveController</servlet-name>
  	<url-pattern>/EmployeeSaveController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>deleteEmployee</servlet-name>
  	<url-pattern>/deleteEmployee</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
